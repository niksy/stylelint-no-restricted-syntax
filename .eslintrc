{
	"root": true,
	"extends": [
		"eslint-config-nitpick",
		"eslint-config-nitpick/typescript",
		"eslint-config-nitpick/next",
		"eslint-config-prettier"
	],
	"plugins": ["eslint-plugin-prettier"],
	"rules": {
		"prettier/prettier": 1
	},
	"overrides": [
		{
			"files": ["rollup.config.js"],
			"parserOptions": {
				"sourceType": "script"
			},
			"rules": {
				"no-console": 0
			}
		}
	]
}
